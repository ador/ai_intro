\ProvidesPackage{beamercolorthemeX}
  [2017/03/02 v0.1 beamer 'X' colour theme]

\mode<presentation>

\definecolor{Xblue}{RGB}{39,84,120}
\definecolor{Xgrey}{RGB}{122,110,100}
\definecolor{Xorange}{RGB}{158,40,0}
\definecolor{Xred}{RGB}{128,39,60}

\setbeamercolor{alerted text}{fg=Xorange}
\setbeamercolor{example text}{fg=Xblue}
\setbeamercolor{structure}{fg=Xblue}

\setbeamercolor{author}{fg=Xgrey}
\setbeamercolor{date}{fg=Xgrey}
\setbeamercolor{framesubtitle}{fg=Xred}

\mode<all>
